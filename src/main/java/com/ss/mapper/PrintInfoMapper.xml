<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ss.mapper.PrintInfoMapper">

	<select id="getPrintInfoByPrintInfoId" resultMap="PrintInfoResultMap">
		select
		<include refid="PrintInfoColumns"></include>
		from t_print_info
		where publish_id = #{publishId}
	</select>

	<resultMap type="PrintInfo" id="PrintInfoResultMap" autoMapping="true">
		</resultMap>

	<sql id="PrintInfoColumns">publish_id,publish_name,publish_date,publish_num</sql>
</mapper>























